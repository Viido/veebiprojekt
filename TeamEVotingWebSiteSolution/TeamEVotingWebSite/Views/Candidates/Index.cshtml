@model IEnumerable<TeamEVotingWebSite.Models.CandidateSet>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Candidate_FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Candidate_Age)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfVotes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Candidate_LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegionSet.Region_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FactionSet.Faction_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Candidate_FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Candidate_Age)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfVotes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Candidate_LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegionSet.Region_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FactionSet.Faction_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Candidate_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Candidate_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Candidate_Id })
        </td>
    </tr>
}

</table>
