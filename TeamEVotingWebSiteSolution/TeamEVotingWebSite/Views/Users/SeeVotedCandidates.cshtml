@model IEnumerable<TeamEVotingWebSite.Models.UserVotedCandidatesVM>

@{
    ViewBag.Title = "SeeVotedCandidates";
}

<h2>SeeVotedCandidates</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            User ID
        </th>
        <th>
            User Firstname
        </th>
        <th>
            User Lastname
        </th>
        <th>
            Candidate ID
        </th>
        <th>
            Candidate Firstname
        </th>
        <th>
            Candidate LastName
        </th>
        <th>
            Candidate Age
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.User_Id
        </td>
        <td>
            @item.User_FirstName
        </td>
        <td>
            @item.User_LastName
        </td>
        <td>
            @item.Candidate_Id
        </td>
        <td>
            @item.Candidate_FirstName
        </td>
        <td>
            @item.Candidate_LastName
        </td>
        <td>
            @item.Candidate_Age
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.User_Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.User_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.User_Id })
        </td>
    </tr>
}

</table>
